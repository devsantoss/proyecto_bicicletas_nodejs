extends ../layout

block content 
    a(href='/bicicletas') Volver 

    br
    br
    br

    h1 Nueva bicicleta 

    br
    br
    
    form(action='/bicicletas/create' method='post')
        label(for="id") ID:
        input#ID(type="text" name='id')
        br
        label(for="color") Color:
        input#ID(type="text" name='color')
        br
        label(for="modelo") Modelo:
        input#ID(type="text" name='modelo')
        br
        label(for="lat") Latitud:
        input#ID(type="text" name='lat')
        br
        label(for="lng") Longitud:
        input#ID(type="text" name='lng')
        br
        br
        input(type="submit", value="OK")